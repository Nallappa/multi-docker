{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions":[
       {
           "name": "client",
           "image": "para27rsh/docker-complex-client",
           "hostname": "client",
           "essential": false
       },
       {
        "name": "server",
        "image": "para27rsh/docker-complex-server",
        "hostname": "api",
        "essential": false
       },
       {
        "name": "worker",
        "image": "para27rsh/docker-complex-worker",
        "hostname": "worker",
        "essential": false
       },
       {
        "name": "nginx",
        "image": "para27rsh/docker-complex-nginx",
        "hostname": "nginx",
        "essential": true,
        "PortMappings" :[{
            "hostPort": 80,
            "containerport": 80
        }],
        "links":["client","server"]
       }
    ]
}